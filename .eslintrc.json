{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true,
        "jest":true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react":{
            "version": "latest"
        }
    },
    "rules": {
        "react/react-in-jsx-scope":"off",
        "react/prop-types": 0,
        "quotes":["error","double"]
    }
}
